{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
      "import/no-unresolved": "off",
      "import/prefer-default-export": "off",
      "no-useless-constructor": "off",
      "class-methods-use-this":"off",
      "no-empty-function":"off",
      "no-unsuded-vars": "off"
    }
}
